pragma solidity ^0.5.1;contract Correction {        address[] cList; // 申请仲裁的交易列表    function addCorrectionList(address _trade) external {        uint8 count = 0;        for (uint i=0; i < cList.length; i++)        {            if (cList[i] == _trade){                count++;            }        }       if (count == 0){            cList.push(_trade);       }            }        //遍历列表确认当前位置    function travel(address _cur) private view returns(uint) {        for (uint i=0; i < cList.length; i++)        {            if (cList[i] == _cur){                return i;            }        }    }        //更新列表，删除已处理项    function update(uint index) private {        if (index >= cList.length) return;        for (uint i = index; i<cList.length-1; i++) {            cList[i] = cList[i+1];        }        delete cList[cList.length-1];        cList.length--;    }        //同意仲裁申请    function accept(address _address) external {        //调用交易函数        update(travel(_address));    }        //否决仲裁申请    function deny(address _address) external {                update(travel(_address));    }        }